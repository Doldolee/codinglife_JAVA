package javaFlowControl;

public class BooleanApp {

	public static void main(String[] args) {
		
		String[] array1 = {"egoing", "new" };
		String inputId = args[0];
		
		boolean isLogined = fasle;
		
		for(int i = 0; i<array1.length; i++) {
