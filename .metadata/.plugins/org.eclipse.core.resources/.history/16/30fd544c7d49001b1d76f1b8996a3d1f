package javaFlowControl;

public class BooleanApp {

	public static void main(String[] args) {
		System.out.println(1>1);
		System.out.println(1);
		
		String id = "egoing";
		String inputId = args[0];
		
		if(id == inputId) {
			System.out.println(1);
		}else {
			System.out.println(2);
		}
		

	}

}
